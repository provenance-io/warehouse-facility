{
    "contractLabel": "warehouse-facility",
    "build": {
        "target": "develop"
    },
    "binding": {
        "name": "sc",
        "root": "pb"
    },
    "isSingleton": false,
    "initArgs": {
        "bind_name": "warehouse-facility.sc.pb",
        "contract_name": "warehouse_facility",
        "facility": {
            "originator": "${provenance::getAddressForKey('originator')}",
            "warehouse": "${provenance::getAddressForKey('warehouse')}",
            "escrow_marker": "${provenance::getMarkerAddress('pb.fse.1')}",
            "marker_denom": "pb.fm.1",
            "stablecoin_denom": "omni.usd",
            "advance_rate": "75.125",
            "paydown_rate": "102.25"
        }
    },
    "env": {
        "keys": [
            {
                "name": "omnibus",
                "minHash": 100000,
                "initialHash": 100000000
            },
            {
                "name": "portfolio_manager",
                "minHash": 100000,
                "initialHash": 100000000
            },
            {
                "name": "originator",
                "minHash": 100000,
                "initialHash": 100000000
            },
            {
                "name": "warehouse",
                "minHash": 100000,
                "initialHash": 100000000
            }
        ],
        "markers": [
            {
                "denom": "omni.usd",
                "supply": 10000000000,
                "manager": "omnibus",
                "type": "coin",
                "grants": [
                    {
                        "key": "omnibus",
                        "privs": [
                            "admin",
                            "burn",
                            "deposit",
                            "delete",
                            "mint",
                            "transfer",
                            "withdraw"
                        ]
                    }
                ]
            },
            {
                "denom": "pb.fse.1",
                "supply": 1,
                "manager": "portfolio_manager",
                "type": "restricted",
                "grants": [
                    {
                        "key": "portfolio_manager",
                        "privs": [
                            "admin",
                            "burn",
                            "deposit",
                            "delete",
                            "mint",
                            "transfer",
                            "withdraw"
                        ]
                    }
                ]
            }
        ]
    }
}